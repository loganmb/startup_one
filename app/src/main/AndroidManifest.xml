<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="br.com.logan.pwf_startupone">

    <application
            android:name=".MyApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/FullScreen">
        <activity android:name=".ui.personal_info.BasicInfoActivity">
        </activity>
        <activity android:name=".ui.form.FormUsuarioActivity">
        </activity>
        <activity android:name=".ui.delete_account.DeleteAcctActivity">
        </activity>
        <activity android:name=".ui.login.LoginActivity">
        </activity>
        <activity android:name=".ui.singup.SignUpActivity">
        </activity>
        <activity android:name=".ui.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.main.MainActivity">
        </activity>
    </application>

</manifest>